{
  "index_name": "test_hazelcast_platform",
  "start_urls": [
    {
      "url": "https://hardcore-allen-f5257d.netlify.app/platform/(?P<version>.*?)/",
      "tags": [
        "platform"
      ],
      "variables": {
        "version": ["5.0-snapshot"]
      },
      "selectors_key": "platform"
    }
  ],
  "sitemap_urls": [
    "https://hardcore-allen-f5257d.netlify.app/sitemap.xml"
  ],
  "stop_urls": [
    "index.html"
  ],
  "selectors": {
    "default": {
      "lvl0": ".doc h1",
      "lvl1": ".doc h2",
      "text": ".doc p, .doc td.content, .doc th.tableblock"
    },
    "platform": {
      "lvl0": {
        "selector": "/html[1]/body[1]/div[1]/main[1]/div[1]/nav[1]/ul[1]/li[1]/a[1]",
        "type": "xpath",
        "global": true,
        "default_value": "Hazelcast"
      },
      "lvl1": {
          "selector": "/html[1]/body[1]/div[1]/main[1]/div[1]/nav[1]/ul[1]/li[2]",
          "type": "xpath",
          "global": true
      },
      "lvl2": ".doc h1",
      "lvl3": ".doc h2",
      "text": ".doc p, .doc td.content, .doc th.tableblock"
    }
  },
  "custom_settings": {
    "attributesForFaceting": [
      "tags"
    ]
  }
}
